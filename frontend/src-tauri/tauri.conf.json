{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Cycle Planner",
  "version": "0.1.0",
  "identifier": "com.local.cycleplanner",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Cycle Planner",
        "width": 1320,
        "height": 860,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": []
  }
}
